{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["DISCORD_TOKEN"]
    },
    "register": {
      "cache": false,
      "env": ["DISCORD_TOKEN", "DISCORD_CLIENT_ID"]
    }
  }
}
